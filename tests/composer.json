{
  "name": "airnz/silverstripe-simplesubsites",
  "type": "silverstripe-module",
  "description": "Run multiple sites from a single SilverStripe install.",
  "license": "BSD-3-Clause",
  "keywords": ["silverstripe", "subsites", "multisite", "multisites"],
  "authors": [
    {
      "name": "Sam Minnee",
      "email": "sam@silverstripe.com"
    },
    {
      "name": "Mike Cochrane",
      "email": "mikec@mikenz.geek.nz"
    }
  ],
  "require":
  {
    "silverstripe/framework": "dev-master",
    "silverstripe/cms": "dev-master",
    "silverstripe/asset-admin": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "extra": {
    "installer-name": "simplesubsites"
  },
  "autoload": {
    "psr-4": {
      "AirNZ\\SimpleSubsites\\": "code/",
      "AirNZ\\SimpleSubsites\\Tests\\": "tests/"
    }
  }
